{
    "name": "react-repick",
    "version": "0.0.1",
    "private": true,
    "description": "A collection of React pickers.",
    "keywords": [
        "react",
        "datepicker",
        "calendar",
        "timepicker",
        "time",
        "date",
        "pick",
        "pickers",
        "pickers",
        "timepickers",
        "datepickers",
        "react-datepicker",
        "react-calendar",
        "react-timepicker",
        "react-time",
        "react-date",
        "react-pick",
        "react-pickers",
        "react-timepickers",
        "react-datepickers"
    ],
    "author": "Bradley Dirheimer",
    "type": "module",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite",
        "format": "prettier --write .",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "prettier": {
        "arrowParens": "always",
        "endOfLine": "auto",
        "plugins": [
            "prettier-plugin-packagejson",
            "prettier-plugin-tailwindcss"
        ],
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
    },
    "dependencies": {
        "date-fns": "^3.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.13",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "clsx": "^2.1.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "prettier-plugin-packagejson": "^2.5.0",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.2.2",
        "vite": "^5.2.0"
    },
    "packageManager": "pnpm@9.1.2+sha256.19c17528f9ca20bd442e4ca42f00f1b9808a9cb419383cd04ba32ef19322aba7"
}
